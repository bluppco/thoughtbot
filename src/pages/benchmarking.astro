---
// IMPORTS BENCHMARKING FROM MD
import { frontmatter as benchmarking } from "@/content/benchmarking/home.mdx"

// IMPORTS ASTRO LAYOUTS
import Layout from "@/layouts/layout/index.astro"
import Container from "@/layouts/container/index.astro"

// IMPORTS ASTRO COMPONENTS
import Footer from "@/components/footer/index.astro"
import Header from "@/components/header/index.astro"
import HeaderBanner from "@/components/home/header-banner/index.astro"
import BenchmarkingJSX from "@/components/benchmarking"
---
<Layout
	title={ "Benchmarking" }
	description={ "Benchmarking" }
	image=""
>
    <HeaderBanner />
    <Header />
    <section class="py-40">
        <Container>
            <BenchmarkingJSX client:load data={ benchmarking }/>
        </Container>
    </section>
    <Footer />
</Layout>
