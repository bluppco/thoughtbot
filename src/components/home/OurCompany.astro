---
// COMPONENTS
import ImageHeadingTitleDescriptionCTAAligned from "../ImageHeadingTitleDescriptionCTAAligned.astro"

const { data } = Astro.props
---
<section class="py-10 md:py-20">
    <ImageHeadingTitleDescriptionCTAAligned
        src={ data.cover }
        alt=""
        heading={ data.title }
        title={ data.sub_title }
        description={ data.description }
        cta={ data.button_title }
    />
</section>
