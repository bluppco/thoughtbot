---

// IMPORTS ASTRO COMPONENTS
import CaseStudies from "@/components/home/case-studies/index.astro"
import Clients from "@/components/home/clients/index.astro"
import Footer from "@/components/common/footer/index.astro"
import Header from "@/components/common/header/index.astro"
import HeaderBanner from "@/components/home/header-banner/index.astro"
import Hero from "@/components/home/hero/index.astro"
import Incubator from "@/components/home/incubator/index.astro"
import LetsTalk from "@/components/home/lets-talk/index.astro"
import OurCompany from "@/components/home/company/index.astro"
import OurServices from "@/components/home/our-services/index.astro"
import Resources from "@/components/home/resources/index.astro"
import ReviewBanner from "@/components/home/review-banner/index.astro"
import ServiceHighlight from "@/components/home/service/index.astro"
import WorkshopsAndEvents from "@/components/home/workshops-and-events/index.astro"

// ASTRO PROPS
const {

    case_studies,
    CDN,
    ENVIRONMENT,
    home_data,
    PROJECT,
    resources

} = Astro.props

---
<HeaderBanner ENVIRONMENT={ ENVIRONMENT } />
<Header ENVIRONMENT={ ENVIRONMENT } />
<Hero
    CDN={ CDN }
    data={ home_data.masthead }
    PROJECT={ PROJECT }
/>
<OurServices data={ home_data.our_services } />
<Clients
    CDN={ CDN }
    data={ home_data.clients }
    PROJECT={ PROJECT }
/>
<ServiceHighlight
    CDN={ CDN }
    data={ home_data.service_highlight }
    PROJECT={ PROJECT }
/>
<CaseStudies
    CDN={ CDN }
    data={ case_studies }
    home={ home_data.case_study }
    PROJECT={ PROJECT }
/>
<ReviewBanner
    CDN={ CDN }
    data={ home_data.review_banner }
    PROJECT={ PROJECT }
/>
<WorkshopsAndEvents
    CDN={ CDN }
    data={ home_data.workshops_and_events }
    PROJECT={ PROJECT }
/>
<OurCompany
    CDN={ CDN }
    data={ home_data.our_company }
    PROJECT={ PROJECT }
/>
<Resources
    data={ resources }
    home={ home_data.resource }
/>
<Incubator
    CDN={ CDN }
    data={ home_data.incubator }
    PROJECT={ PROJECT }
/>
<LetsTalk data={ home_data.lets_talk } />
<Footer />
